<!DOCTYPE html>
<html lang="en" class="page-{{slug_section}}-{{slug_chapter}}">
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="msapplication-tap-highlight" content="no">
    <link rel="shortcut icon" type="image/png" href="{{assetsFix assets}}/img/favicon.png">
    <meta name="description" content="{{chapter}} | {{ description }}">
    <title>{{chapter}} | {{ site.title }}</title>

    <!-- CSS-->
    <link href="{{assetsFix assets}}/css/bas-material-ui.css?v=0.0.0" rel="stylesheet">
    <link href="{{assetsFix assets}}/css/bas-material-ui-docs.css?v=0.0.0" rel="stylesheet">
    <!-- ADS-ASYNC -->
    <!-- ADS-PAGE-LEVEL -->
</head>
<body class="bas-ui-layout-header-fixed bas-ui-side-nav-auto bas-ui-side-nav-hide-off bas-ui-side-nav-with-overlay-off">
    <!--[if lt IE 9]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->

    <!-- Header -->
    {{> header }}

    <main class="bas-ui-main bas-ui-scrollbar-y">
        {{> body }}

        <!-- Footer -->
        {{> footer }}
    </main>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.0.0/jquery.min.js"></script>
    <script src="{{assetsFix assets}}/js/bas-material-ui.js?v=0.0.0"></script>
    <script src="{{assetsFix assets}}/js/bas-material-ui-docs.js?v=0.0.0"></script>

    <!-- Google Analytics -->
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-XXXXX-X', 'bas-material-ui.basgrani.com');
        ga('send', 'pageview');
